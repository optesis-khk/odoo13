<odoo>
  <data>
          <record id="sale_order_form_inherit_bis" model="ir.ui.view">
              <field name="name">sale.order.form.inherit.bis</field>
              <field name="model">sale.order</field>
              <field name="inherit_id" ref="sale.view_order_form"/>
              <field name="arch" type="xml">
                <xpath expr="//field[@name='date_order']" position="after">
                    <field name="date_order_immutable" invisible="False"/>
                </xpath>
              </field>
            </record>
  </data>
</odoo>